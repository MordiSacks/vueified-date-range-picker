{
  "name":            "vueified-date-range-picker",
  "version":         "0.1.2",
  "description":     "A vue date range picker",
  "main":            "src/VueifiedDateRangePicker.vue",
  "keywords":        [
    "vue",
    "vuejs",
    "vue-date-range-picker",
    "date picker",
    "date range",
    "daterange",
    "date range picker"
  ],
  "author":          "Mordi Sacks",
  "contributors":    [
    {
      "name": "Racheli Elias-Sacks"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MordiSacks/vueified-date-range-picker.git"
  },
